{
  "app": {
    "name": "Ultra Pinnacle AI Studio",
    "version": "1.0.0",
    "host": "0.0.0.0",
    "port": 8000,
    "debug": false,
    "log_level": "INFO",
    "env": "development"
  },
  "models": {
    "default_model": "llama-2-7b-chat",
    "llama-2-7b-chat": {
      "path": "models/llama-2-7b-chat.ggmlv3.q4_0.bin",
      "type": "llama",
      "max_tokens": 2048,
      "temperature": 0.7
    },
    "stable-diffusion": {
      "path": "models/stable-diffusion-v1-5.ckpt",
      "type": "diffusion",
      "height": 512,
      "width": 512,
      "num_inference_steps": 50
    }
  },
  "security": {
    "secret_key": "${JWT_SECRET}",
    "algorithm": "HS256",
    "access_token_expire_minutes": 30,
    "cors_origins": ["http://localhost:3000", "http://localhost:8080", "http://127.0.0.1:3000"],
    "allowed_hosts": ["*"],
    "rate_limit_per_minute": 60
  },
  "paths": {
    "models_dir": "models/",
    "encyclopedia_dir": "encyclopedia/",
    "uploads_dir": "uploads/",
    "logs_dir": "logs/"
  },
  "database": {
    "url": "sqlite:///./ultra_pinnacle.db",
    "echo": false
  },
  "features": {
    "authentication": true,
    "file_upload": true,
    "chat_mode": true,
    "code_generation": true,
    "image_generation": false,
    "database_persistence": true
  }
}